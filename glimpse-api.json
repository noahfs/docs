                "all",
                "5yr",
                "1yr",
                "90d",
                "30d",
                "7d",
                "1d",
                "4h",
                "1h"
              ]
            }
          },
          {
            "in": "query",
            "name": "geo",
            "required": false,
            "schema": {
              "type": "string",
              "default": "",

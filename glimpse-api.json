              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "422": {
            "$ref": "#/components/responses/UNPROCESSABLE_ENTITY"
          },
          "default": {
            "$ref": "#/components/responses/DEFAULT_ERROR"
          }
        }
      }
    },
    "/v1/interest_enriched": {
      "get": {
        "parameters": [
          {
            "in": "query",
            "name": "keyword",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "phrase_match",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "in": "query",
            "name": "timeline",
            "required": false,
            "schema": {
              "type": "string",
              "default": "5yr",
              "enum": ["5yr"]
            }
          },
          {
            "in": "query",
            "name": "geo",
            "required": false,
            "schema": {
              "type": "string",
              "default": "",
              "enum": [
                "AD","AE","AG","AL","AM","AO","AR","AT","AU","AZ","BA","BB","BD","BE","BG","BH","BJ","BN","BO","BR","BS","BT","BW","BY","BZ","CA","CD","CH","CI","CL","CM","CN","CO","CR","CU","CV","CY","CZ","DE","DK","DM","DO","DZ","EC","EE","EG","ES","ET","FI","FJ","FR","GA","GB","GD","GE","GH","GM","GQ","GR","GT","GY","HK","HN","HR","HT","HU","ID","IE","IL","IN","IQ","IR","IS","IT","JM","JO","JP","KE","KG","KH","KI","KR","KW","KZ","LA","LB","LC","LK","LT","LU","LV","LY","MA","MD","ME","MG","MK","MM","MN","MO","MT","MU","MV","MX","MY","MZ","NA","NE","NG","NI","NL","NO","NP","NZ","OM","PA","PE","PH","PK","PL","PR","PT","PY","QA","RO","RS","RU","RW","SA","SB","SC","SD","SE","SG","SI","SK","SN","SR","ST","SV","SY","SZ","TH","TM","TN","TO","TR","TT","TW","TZ","UA","UG","US","UY","UZ","VC","VE","VN","VU","WS","XK","ZA","ZW",""
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "422": {
            "$ref": "#/components/responses/UNPROCESSABLE_ENTITY"
          }
        }
      }
    },
    "/v1/volume": {
      "get": {
        "parameters": [
          {
            "in": "query",
            "name": "keyword",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "in": "query",
            "name": "phrase_match",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": false
            }
          },
          {
            "in": "query",
            "name": "geo",
            "required": false,
            "schema": {
              "type": "string",
              "default": "",
              "enum": [
                "AD","AE","AG","AL","AM","AO","AR","AT","AU","AZ","BA","BB","BD","BE","BG","BH","BJ","BN","BO","BR","BS","BT","BW","BY","BZ","CA","CD","CH","CI","CL","CM","CN","CO","CR","CU","CV","CY","CZ","DE","DK","DM","DO","DZ","EC","EE","EG","ES","ET","FI","FJ","FR","GA","GB","GD","GE","GH","GM","GQ","GR","GT","GY","HK","HN","HR","HT","HU","ID","IE","IL","IN","IQ","IR","IS","IT","JM","JO","JP","KE","KG","KH","KI","KR","KW","KZ","LA","LB","LC","LK","LT","LU","LV","LY","MA","MD","ME","MG","MK","MM","MN","MO","MT","MU","MV","MX","MY","MZ","NA","NE","NG","NI","NL","NO","NP","NZ","OM","PA","PE","PH","PK","PL","PR","PT","PY","QA","RO","RS","RU","RW","SA","SB","SC","SD","SE","SG","SI","SK","SN","SR","ST","SV","SY","SZ","TH","TM","TN","TO","TR","TT","TW","TZ","UA","UG","US","UY","UZ","VC","VE","VN","VU","WS","XK","ZA","ZW",""
              ]
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          },
          "422": {
            "$ref": "#/components/responses/UNPROCESSABLE_ENTITY"
          },
          "default": {
            "$ref": "#/components/responses/DEFAULT_ERROR"
          }
        }
      }
    }
  },
  "info": {
    "title": "Glimpse Enterprise API",
    "version": "v1"
  },
  "openapi": "3.0.2",
  "components": {
    "schemas": {
      "Error": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Error name"
          },
          "code": {
            "type": "integer",
            "description": "Error code"
          },
          "message": {
            "type": "string",
            "description": "Error message"
          },
          "errors": {
            "type": "object",
            "description": "Errors"
          }
        }
      },
      "PaginationMetadata": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer"
          },
          "total_pages": {
            "type": "integer"
          },
          "first_page": {
            "type": "integer"
          },
          "last_page": {
            "type": "integer"
          },
          "page": {
            "type": "integer"
          },
          "previous_page": {
            "type": "integer"
          },
          "next_page": {
            "type": "integer"
          }
        }
      },
      "Prompt": {
        "type": "object",
        "properties": {
          "refresh": {
            "type": "boolean",
            "default": false
          },
          "prompt": {
            "type": "string"
          }
        }
      },
      "DBKeywords": {
        "type": "object",
        "properties": {
          "keywords": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TimeseriesPost": {
        "type": "object",
        "properties": {
          "timeseries": {
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "window": {
            "type": "integer",
            "default": 4
          },
          "threshold": {
            "type": "integer",
            "default": 2
          }
        }
      },
      "TimeseriesInt": {
        "type": "object",
        "properties": {
          "timeseries": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        }
      },
      "BreakoutTrend": {
        "type": "object",
        "properties": {
          "categories": {
            "nullable": true
          },
          "longtail_preview": {
            "nullable": true
          },
          "slope": {
            "type": "number",
            "nullable": true
          },
          "keyword": {
            "type": "string",
            "maxLength": 255
          },
          "ts": {
            "nullable": true
          },
          "volume": {
            "type": "number",
            "nullable": true
          },
          "rank": {
            "type": "integer",
            "nullable": true
          },
          "headline": {
            "type": "string",
            "nullable": true
          }
        },
        "required": ["keyword"]
      }
    },
    "responses": {
      "UNPROCESSABLE_ENTITY": {
        "description": "Unprocessable Entity",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "DEFAULT_ERROR": {
        "description": "Default error response",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "headers": {
      "PAGINATION": {
        "description": "Pagination metadata",
        "schema": {
          "$ref": "#/components/schemas/PaginationMetadata"
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "apikey"
      }
    }
  },
  "servers": [
    {
      "url": "https://enterprise.meetglimpse.com/"
    }
  ],
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "x-readme": {
    "explorer-enabled": true,
    "proxy-enabled": true
  }
}

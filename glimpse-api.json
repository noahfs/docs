  "info": {
    "title": "Glimpse Enterprise API",
    "version": "v1"
  },
  "openapi": "3.0.2",
  "components": {
    "schemas": {
      "Error": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Error name"
          },
          "code": {
            "type": "integer",
            "description": "Error code"
          },
          "message": {
            "type": "string",
            "description": "Error message"
          },
          "errors": {
            "type": "object",
            "description": "Errors"
          }
        }
      },
      "PaginationMetadata": {
        "type": "object",
        "properties": {
          "total": {
            "type": "integer"
          },
          "total_pages": {
            "type": "integer"
          },
          "first_page": {
            "type": "integer"
          },
          "last_page": {
            "type": "integer"
          },
          "page": {
            "type": "integer"
          },
          "previous_page": {
            "type": "integer"
          },
          "next_page": {
            "type": "integer"
          }
        }
      },
      "Prompt": {
        "type": "object",
        "properties": {
          "refresh": {
            "type": "boolean",
            "default": false
          },
          "prompt": {
            "type": "string"
          }
        }
      },
      "DBKeywords": {
        "type": "object",
        "properties": {
          "keywords": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "TimeseriesPost": {
        "type": "object",
        "properties": {
          "timeseries": {
            "type": "array",
            "items": {
              "type": "number"
            }
          },
          "window": {
            "type": "integer",
            "default": 4
          },
          "threshold": {
            "type": "integer",
            "default": 2
          }
        }
      },
      "TimeseriesInt": {
        "type": "object",
        "properties": {
          "timeseries": {
            "type": "array",
            "items": {
              "type": "integer"
            }
          }
        }
      },
      "BreakoutTrend": {
        "type": "object",
        "properties": {
          "categories": {
            "nullable": true
          },
          "longtail_preview": {
            "nullable": true
          },
          "slope": {
            "type": "number",
            "nullable": true
          },
          "keyword": {
            "type": "string",
            "maxLength": 255
          },
          "ts": {
            "nullable": true
          },
          "volume": {
            "type": "number",
            "nullable": true
          },
          "rank": {
            "type": "integer",
            "nullable": true
          },
          "headline": {
            "type": "string",
            "nullable": true
          }
        },
        "required": ["keyword"]
      }
    },
    "responses": {
      "UNPROCESSABLE_ENTITY": {
        "description": "Unprocessable Entity",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      },
      "DEFAULT_ERROR": {
        "description": "Default error response",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/Error"
            }
          }
        }
      }
    },
    "headers": {
      "PAGINATION": {
        "description": "Pagination metadata",
        "schema": {
          "$ref": "#/components/schemas/PaginationMetadata"
        }
      }
    },
    "securitySchemes": {
      "ApiKeyAuth": {
        "type": "apiKey",
        "in": "header",
        "name": "apikey"
      }
    }
  },
  "servers": [
    {
      "url": "https://enterprise.meetglimpse.com/"
    }
  ],
  "security": [
    {
      "ApiKeyAuth": []
    }
  ],
  "x-readme": {
    "explorer-enabled": true,
    "proxy-enabled": true
  }
